{
  "entryFile": "src/start-up/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controllers/admin/**/*.ts"],
  "spec": {
    "outputDirectory": "src/routes/admin",
    "specVersion": 3,
    "basePath": "api",
    "specFileBaseName": "swagger",
    "securityDefinitions": {
    "BearerAuth": {
      "type": "http",
      "scheme": "bearer",
      "in": "header",
      "name": "Authorization"
    }
  },
    "security": [
      { "BearerAuth": [] }
  ]
  },
  "routes": {
    "routesDir": "src/routes/admin",
    "authenticationModule": "src/start-up/swagger.ts"
  }
}
